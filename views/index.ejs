  <div class="modal" id="signUpModal" tabindex="1" role="dialog" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h1> Sign Up</h1>
          <button type="button" class="close" data-dismiss="modal" aria-label="close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div class="row">
            <form action="/signup" method="POST" class="form-admin" encType="multipart/form-data">

              <div class="form-group row">
                <label for="userName" class="col-sm-2 col-form-label" style="color: white;  margin-right: 5px;">Email</label>
                <div class="col-sm-12">
                  <input type="name" class="form-control" name="userName" id="name" placeholder="Votre nom" style="border-radius: 2px">
                </div>
              </div>

              <div class="form-group row">
                <label for="userEmail" class="col-sm-2 col-form-label" style="color: white;  margin-right: 5px;">Email</label>
                <div class="col-sm-12">
                  <input type="email" class="form-control" name="userEmail" id="email" placeholder="Votre email" style="border-radius: 2px">
                </div>
              </div>

              <div class="form-group row">
                <label for="userPassword" class="col-sm-6 col-form-label" style="color: white; margin-right: 5px;">Mot de passe</label>
                <div class="col-sm-12">
                  <input type="password" class="form-control" name="userPassword" id="password" placeholder="Votre mot de passe" style="border-radius: 2px">
                </div>
              </div>

              <br>
              <button type="submit" class="btn-admin mb-2 btn-light">Register</button>
            </form>
          </div>
        </div>

      </div>
    </div>
  </div>


  <div class="modal" id="loginModal" tabindex="1" role="dialog" aria-hidden="true">
    <div class="modal-dialog" role="document">
