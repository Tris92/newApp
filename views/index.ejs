          <h1> Log In</h1>
          <button type="button" class="close" data-dismiss="modal" aria-label="close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div class="row">
            <form action="/login" method="POST" class="form-admin" encType="multipart/form-data">

              <div class="form-group row">
                <label for="userEmail" class="col-sm-2 col-form-label" style="color: white;  margin-right: 5px;">Email</label>
                <div class="col-sm-12">
                  <input type="email" class="form-control" name="userEmail" id="email" placeholder="Votre email" style="border-radius: 2px">
                </div>
              </div>

              <div class="form-group row">
                <label for="userPassword" class="col-sm-6 col-form-label" style="color: white; margin-right: 5px;">Mot de passe</label>
                <div class="col-sm-12">
                  <input type="password" class="form-control" name="userPassword" id="password" placeholder="Votre mot de passe" style="border-radius: 2px">
                </div>
              </div>

              <br>
              <button type="submit" class="btn-admin mb-2 btn-light">Se connecter</button>
            </form>
          </div>
        </div>

      </div>
